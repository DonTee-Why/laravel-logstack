{
    "name": "donteewhy/laravel-logstack",
    "description": "A custom logging driver for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "role": "Developer",
        "name": "Timilehin Olusegun",
        "email": "timiddon97@gmail.com",
        "homepage": "https://github.com/DonTee-Why"
    }],
    "autoload": {
        "psr-4": {
            "DonTeeWhy\\LogStack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DonTeeWhy\\LogStack\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/log": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/config": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/queue": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "monolog/monolog": "^2.0|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "phpstan/phpstan": "^1.10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
